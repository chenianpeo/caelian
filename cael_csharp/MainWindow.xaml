<Window x:Class="cael_csharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cael_csharp"
        mc:Ignorable="d"
        Title="Caelian" Height="450" Width="800">
    <Grid Margin="5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <StackPanel>
            <TextBlock HorizontalAlignment="Center">Cael</TextBlock>
            <Button Margin="0 10" Click="Button_Click">AppAdd</Button>
            <Button Click="Button_Click_1">Clear</Button>
        </StackPanel>
        <ListBox Grid.Column="1" Margin="5" x:Name="boxPrint"/>
    </Grid>
</Window>
